@model IEnumerable<SachOnline.Models.NhaXuatBan>

<ul class="list-group">
    <li class="list-group-item active" style="font-weight:bold; text-align:center">SÁCH THEO NHÀ XUẤT BẢN</li>


    @foreach (var item in Model)
    {

        <a href="@Url.Action("SachTheoNhaXuatBan", "SachOnline", new { id = item.NhaXuatBanID })"
           class="list-group-item">@item.NhaXuatBanName</a>
    }
</ul>



